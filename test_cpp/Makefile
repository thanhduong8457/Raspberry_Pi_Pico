#########################################
#				My Makefile				#
#########################################

GCC = g++

delta:
	@$(GCC) -o ouput delta.cpp -I./*
	@./ouput
	@rm -rf ouput

main:
	@$(GCC) -o ouput main.cpp
	@./ouput
	@rm -rf ouput

struct_vector:
	@$(GCC) -o ouput struct_vector.c
	@./ouput
	@rm -rf ouput

run:
	@./ouput
	
clean:
	@rm -rf ouput